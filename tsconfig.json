{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "declaration": false,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "strictFunctionTypes": false,
    "emitDeclarationOnly": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
        "ES2022",
        "dom"
    ]
  },
  "exclude": ["node_modules", "dist"]
}